    MessagingInstance.addUserData(
      'apt update',
      'apt install -y awscli',
      `daemonToken=$(aws secretsmanager get-secret-value --secret-id ${secret.secretName} --query SecretString --region ${region} --output text)`,
      'touch replicated.conf',
      `json="{\\\"DaemonToken\\\": \\\"$daemonToken\\\"}"`,
      'echo $json > replicated.conf',
      'mv replicated.conf /etc/replicated.conf',
      'curl -sSL -o install.sh https://get.replicated.com/docker/wickrenterprise/stable',
      'DEBIAN_FRONTEND=noninteractive READ_TIMEOUT=\"-t 1\" bash ./install.sh'
    );


    VoiceVideoInstance.addUserData(
      'apt update',
      'apt install -y awscli',
      `daemonToken=$(aws secretsmanager get-secret-value --secret-id ${secret.secretName} --query SecretString --region ${region} --output text)`,
      'TOKEN=`curl -X PUT "http://169.254.169.254/latest/api/token" -H "X-aws-ec2-metadata-token-ttl-seconds: 21600"`',
      'VoiceVideoPrivateIP=$(curl http://169.254.169.254/latest/meta-data/local-ipv4)',
      'VoiceVideoPublicIP=$(curl http://169.254.169.254/latest/meta-data/public-ipv4)',
      `curl -sSL https://get.replicated.com/operator/wickrenterprise/stable | sudo DEBIAN_FRONTEND=noninteractive bash -s no-proxy daemon-endpoint=${MessagingInstance.instancePrivateIp}:9879 daemon-token=$daemonToken private-address=$VoiceVideoPrivateIP public-address=$VoiceVideoPublicIP tags=calling`
    );


    ComplianceInstance.addUserData(
      'apt update',
      'apt install -y awscli',
      'apt install -y apt-transport-https',
      'apt install -y ca-certificates curl gnupg lsb-release',
      'apt install -y software-properties-common',
      'rm -f /usr/share/keyrings/docker-archive-keyring.gpg',
      'curl -fsSL https://download.docker.com/linux/ubuntu/gpg | sudo gpg --dearmor -o /usr/share/keyrings/docker-archive-keyring.gpg',
      "echo \"deb [arch=$(dpkg --print-architecture) signed-by=/usr/share/keyrings/docker-archive-keyring.gpg] https://download.docker.com/linux/ubuntu $(lsb_release -cs) stable\" | sudo tee /etc/apt/sources.list.d/docker.list > /dev/null",
      'apt update',
      'apt install -y docker-ce docker-ce-cli containerd.io',
      'systemctl enable docker',
      'systemctl start docker',
      'usermod -aG docker ubuntu',
      'mkdir /opt/WickrIO',
      'docker pull wickr/bot-enterprise:5.68.14.03',
      'docker run -v /opt/WickrIO:/opt/WickrIO -d --name wickr-compliance --restart=always -ti wickr/bot-enterprise:5.68.14.03',
      '#docker attach wickr-compliance'
    );

